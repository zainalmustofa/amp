  <main id="content" role="main" class="">
    <article class="form-quote">
      <header>
        <h2 class="mb1 px3 text-center">Pesan Jasa Service AC Terpercaya</h2>
      </header>
      <div class="wrapper">
        <div class="workings-side">
          <h3 class="text-center">Bagaimana Cara Kerjanya?</h3>
          <div class="working">
            <div>
              <amp-img src="<%= image_path("home2/work-1.jpg")%>" width="683" height="1024" layout="responsive" alt="strawberries" class="img-circle mb4 mx3"></amp-img>
            </div>
            <div class="step-description">
              <h5><b>Jelaskan Kebutuhan Anda</b></h5>
              <div class="small">Beritahu kami secara lengkap tentang layanan yang Anda butuhkan</div>
            </div>
          </div>
          <br>
          <div class="working">
            <div>
              <amp-img src="<%= image_path("home2/work-2.jpg")%>" width="683" height="1024" layout="responsive" alt="strawberries" class="img-circle mb4 mx3"></amp-img>
            </div>
            <div class="step-description">
              <h5><b>Tunggu Beberapa Saat</b></h5>
              <div class="small">Penyedia jasa akan langsung menghubungi Anda</div>
            </div>
          </div>
          <br>
          <div class="working">
            <div>
              <amp-img src="<%= image_path("home2/work-3.jpg")%>" width="683" height="1024" layout="responsive" alt="strawberries" class="img-circle mb4 mx3"></amp-img>
            </div>
            <div class="step-description">
              <h5><b>Jaminan Kepuasan</b></h5>
              <div class="small">Dapatkan garansi untuk layanan yang Anda pesan</div>
            </div>
          </div>
        </div>
      </div>

      <section class="px3 mb4">
        <h2 class="">Request Details</h2>
        <p class="red_color mb2">
          <i>
              <b>Kolom bertanda * wajib diisi</b>
          </i>
        </p>
      
        <form method="post" action-xhr="/amp/service_ac_requests" target="_top">
          <div style="margin-bottom: 5px;" class="form-group radio_buttons required service_ac_request_question1">
            <label class="radio_buttons required">
              <abbr title="required">*</abbr> Keluhan atau Masalah pada AC Anda?
            </label>
            <input type="hidden" name="service_ac_request[question1]" value="">
            <span class="radio">
              <label for="service_ac_request_question1_ac_tidak_dingin"><input class="radio_buttons required" trequired="required" type="radio" value="AC tidak dingin" name="service_ac_request[question1]" id="service_ac_request_question1_ac_tidak_dingin">AC tidak dingin</label>
            </span>
            <span class="radio">
              <label for="service_ac_request_question1_ac_berisik">
                <input class="radio_buttons required" required="required" type="radio" value="AC berisik" name="service_ac_request[question1]" id="service_ac_request_question1_ac_berisik">AC berisik
              </label>
            </span>
            <span class="radio">
              <label for="service_ac_request_question1_ac_bocor">
                <input class="radio_buttons required" required="required" type="radio" value="AC bocor" name="service_ac_request[question1]" id="service_ac_request_question1_ac_bocor">AC bocor
              </label>
            </span>
            <span class="radio">
              <label for="service_ac_request_question1_lainnya">
                <input class="radio_buttons required" required="required" type="radio" value="Lainnya" name="service_ac_request[question1]" id="service_ac_request_question1_lainnya">Lainnya
              </label>
            </span>
            <span class="radio">
              <label for="service_ac_request_question1_tidak_ada">
                <input class="radio_buttons required" required="required" type="radio" value="Tidak ada" name="service_ac_request[question1]" id="service_ac_request_question1_tidak_ada">Tidak ada
              </label>
            </span>
          </div>
      
          <div style="margin-bottom: 5px;" class="form-group radio_buttons required service_ac_request_question2">
            <label class="radio_buttons required">
              <abbr title="required">*</abbr> Layanan yang Anda butuhkan?</label>
            <input type="hidden" name="service_ac_request[question2]" value="">
            <span class="radio">
              <label for="service_ac_request_question2_cuci_ac"><input class="radio_buttons required" required="required" type="radio" value="Cuci AC" name="service_ac_request[question2]" id="service_ac_request_question2_cuci_ac">Cuci AC</label>
            </span>
            <span class="radio">
              <label for="service_ac_request_question2_perbaikan_ac"><input class="radio_buttons required" required="required" type="radio" value="Perbaikan AC" name="service_ac_request[question2]" id="service_ac_request_question2_perbaikan_ac">Perbaikan AC</label>
            </span>
            <span class="radio">
              <label for="service_ac_request_question2_tambahisi_freon"><input class="radio_buttons required" required="required" type="radio" value="Tambah/isi freon" name="service_ac_request[question2]" id="service_ac_request_question2_tambahisi_freon">Tambah/isi freon</label></span>
            <span class="radio">
              <label for="service_ac_request_question2_bongkar_pasang_ac"><input class="radio_buttons required" required="required" type="radio" value="Bongkar pasang AC" name="service_ac_request[question2]" id="service_ac_request_question2_bongkar_pasang_ac">Bongkar pasang AC</label>
            </span>
            <span class="radio">
              <label for="service_ac_request_question2_bongkar_ac"><input class="radio_buttons required" required="required" type="radio" value="Bongkar AC" name="service_ac_request[question2]" id="service_ac_request_question2_bongkar_ac">Bongkar AC</label>
            </span>
            <span class="radio">
              <label for="service_ac_request_question2_pasang_ac"><input class="radio_buttons required" required="required" type="radio" value="Pasang AC" name="service_ac_request[question2]" id="service_ac_request_question2_pasang_ac">Pasang AC</label>
            </span>
            <span class="radio">
              <label for="service_ac_request_question2_lainnya"><input class="radio_buttons required" required="required" type="radio" value="Lainnya" name="service_ac_request[question2]" id="service_ac_request_question2_lainnya">Lainnya</label>
            </span>
          </div>
      
          <div class="form-group check_boxes required service_ac_request_question3">
            <label class="check_boxes required"><abbr title="required">*</abbr> Jenis AC yang Anda miliki?</label>
            <input type="hidden" name="service_ac_request[question3][]" value="">
            <span class="checkbox"><label for="service_ac_request_question3_ac_split"><input class="check_boxes required" aria-required="true" type="checkbox" value="AC Split" name="service_ac_request[question3][]" id="service_ac_request_question3_ac_split">AC Split</label></span>
            <span class="checkbox">
              <label for="service_ac_request_question3_ac_central"><input class="check_boxes required" aria-required="true" type="checkbox" value="AC Central" name="service_ac_request[question3][]" id="service_ac_request_question3_ac_central">AC Central</label>
            </span>
            <span class="checkbox">
              <label for="service_ac_request_question3_ac_window"><input class="check_boxes required" aria-required="true" type="checkbox" value="AC Window" name="service_ac_request[question3][]" id="service_ac_request_question3_ac_window">AC Window</label>
            </span>
            <span class="checkbox">
              <label for="service_ac_request_question3_lainnya"><input class="check_boxes required" aria-required="true" type="checkbox" value="Lainnya" name="service_ac_request[question3][]" id="service_ac_request_question3_lainnya">Lainnya</label>
            </span>
            <span class="checkbox">
              <label for="service_ac_request_question3_saya_tidak_tahu"><input class="check_boxes required" aria-required="true" type="checkbox" value="Saya tidak tahu" name="service_ac_request[question3][]" id="service_ac_request_question3_saya_tidak_tahu">Saya tidak tahu</label>
            </span>
          </div>
      
          <div style="margin-bottom: 5px;" class="form-group check_boxes optional service_ac_request_question4">
            <label class="check_boxes optional">Berapa Horsepower (PK) AC Anda?</label>
            <input type="hidden" name="service_ac_request[question4][]" value=""><span class="checkbox"><label for="service_ac_request_question4_12_pk"><input class="check_boxes optional" type="checkbox" value="1/2 PK" name="service_ac_request[question4][]" id="service_ac_request_question4_12_pk">1/2 PK</label></span>
            <span class="checkbox">
              <label for="service_ac_request_question4_34_pk"><input class="check_boxes optional" type="checkbox" value="3/4 PK" name="service_ac_request[question4][]" id="service_ac_request_question4_34_pk">3/4 PK</label>
            </span>
            <span class="checkbox">
              <label for="service_ac_request_question4_1_pk"><input class="check_boxes optional" type="checkbox" value="1 PK" name="service_ac_request[question4][]" id="service_ac_request_question4_1_pk">1 PK</label>
            </span>
            <span class="checkbox">
              <label for="service_ac_request_question4_15_pk"><input class="check_boxes optional" type="checkbox" value="1.5 PK" name="service_ac_request[question4][]" id="service_ac_request_question4_15_pk">1.5 PK</label>
            </span>
            <span class="checkbox">
              <label for="service_ac_request_question4_2_pk"><input class="check_boxes optional" type="checkbox" value="2 pk" name="service_ac_request[question4][]" id="service_ac_request_question4_2_pk">2 pk</label>
            </span>
            <span class="checkbox">
              <label for="service_ac_request_question4_lainnya"><input class="check_boxes optional" type="checkbox" value="Lainnya" name="service_ac_request[question4][]" id="service_ac_request_question4_lainnya">Lainnya</label>
            </span>
          </div>
      
          <div style="margin-bottom: 5px;" class="form-group check_boxes optional service_ac_request_question5">
            <label class="check_boxes optional">Merek AC Anda?</label>
            <input type="hidden" name="service_ac_request[question5][]" value="">
            <span class="checkbox">
              <label for="service_ac_request_question5_lg"><input class="check_boxes optional" type="checkbox" value="LG" name="service_ac_request[question5][]" id="service_ac_request_question5_lg">LG</label>
            </span>
            <span class="checkbox">
              <label for="service_ac_request_question5_daikin"><input class="check_boxes optional" type="checkbox" value="Daikin" name="service_ac_request[question5][]" id="service_ac_request_question5_daikin">Daikin</label>
            </span>
            <span class="checkbox">
              <label for="service_ac_request_question5_sharp"><input class="check_boxes optional" type="checkbox" value="Sharp" name="service_ac_request[question5][]" id="service_ac_request_question5_sharp">Sharp</label>
            </span>
            <span class="checkbox">
              <label for="service_ac_request_question5_fujitsu"><input class="check_boxes optional" type="checkbox" value="Fujitsu" name="service_ac_request[question5][]" id="service_ac_request_question5_fujitsu">Fujitsu</label>
            </span>
            <span class="checkbox">
              <label for="service_ac_request_question5_panasonic"><input class="check_boxes optional" type="checkbox" value="Panasonic" name="service_ac_request[question5][]" id="service_ac_request_question5_panasonic">Panasonic</label>
            </span>
            <span class="checkbox">
              <label for="service_ac_request_question5_lainnya"><input class="check_boxes optional" type="checkbox" value="Lainnya" name="service_ac_request[question5][]" id="service_ac_request_question5_lainnya">Lainnya</label></span>
          </div>
      
          <div class="form-group integer required service_ac_request_question6">
            <label class="control-label integer required" for="service_ac_request_question6">
              <abbr title="required">*</abbr> Berapa unit AC yang perlu dikerjakan?
            </label>
            <div class="ampstart-input inline-block relative m0 p0 mb3 ">
                <input type="number" value="" name="service_ac_request[question6]" id="ip1" class="block border-none p0 m0" placeholder="1">
            </div>
          </div>
      
          <div class="form-group radio_buttons required service_ac_request_question7">
            <label class="radio_buttons required"><abbr title="required">*</abbr> Apakah penyedia jasa perlu membawa tangga?
            </label>
            <input type="hidden" name="service_ac_request[question7]" value="">
            <span class="radio"><label for="service_ac_request_question7_ya">
              <input class="radio_buttons required" required="required" aria-required="true" type="radio" value="Ya" name="service_ac_request[question7]" id="service_ac_request_question7_ya">Ya</label>
            </span>
            <span class="radio"><label for="service_ac_request_question7_tidak">
              <input class="radio_buttons required" required="required" aria-required="true" type="radio" value="Tidak" name="service_ac_request[question7]" id="service_ac_request_question7_tidak">Tidak</label>
            </span>
          </div>
      
          <div class="form-group radio_buttons required service_ac_request_question8">
            <label class="radio_buttons required"><abbr title="required">*</abbr> Apakah jenis properti anda?
            </label>
            <input type="hidden" name="service_ac_request[question8]" value="">
            <span class="radio">
              <label for="service_ac_request_question8_rumah"><input class="radio_buttons required" required="required" aria-required="true" type="radio" value="Rumah" name="service_ac_request[question8]" id="service_ac_request_question8_rumah">Rumah</label>
            </span>
            <span class="radio">
              <label for="service_ac_request_question8_apartment"><input class="radio_buttons required" required="required" aria-required="true" type="radio" value="Apartment" name="service_ac_request[question8]" id="service_ac_request_question8_apartment">Apartment</label>
            </span>
            <span class="radio">
              <label for="service_ac_request_question8_kantor"><input class="radio_buttons required" required="required" aria-required="true" type="radio" value="Kantor" name="service_ac_request[question8]" id="service_ac_request_question8_kantor">Kantor</label>
            </span>
            <span class="radio">
              <label for="service_ac_request_question8_ruko"><input class="radio_buttons required" required="required" aria-required="true" type="radio" value="Ruko" name="service_ac_request[question8]" id="service_ac_request_question8_ruko">Ruko</label>
            </span>
            <span class="radio">
              <label for="service_ac_request_question8_lainnya"><input class="radio_buttons required" required="required" aria-required="true" type="radio" value="Lainnya" name="service_ac_request[question8]" id="service_ac_request_question8_lainnya">Lainnya</label>
            </span>
          </div>
      
          <div class="form-group select required service_ac_request_question10">
            <label class="control-label select required" for="service_ac_request_question10">
            <abbr title="required">*</abbr> Kapan Anda membutuhkan layanan?</label>
            <div class="ampstart-input inline-block relative mb3">
                <input type="date" value="" name="service_ac_request[question9]">
            </div>
          </div>
      
          <div class="form-group select required service_ac_request_question10">
            <label class="control-label select required" for="service_ac_request_question10">
            <abbr title="required">*</abbr> Pada pukul berapa Anda membutuhkan layanan?</label>
            <div class="ampstart-input inline-block relative mb3">
                <select class="form-control select required range-time" required="required" aria-required="true" name="service_ac_request[question10]" id="service_ac_request_question10"><option value=""></option>
                <option value="09:00 - 12:00">09:00 - 12:00</option>
                <option value="12:00 - 15:00">12:00 - 15:00</option>
                <option value="15:00 - 18:00">15:00 - 18:00</option>
                <option value="18:00 - 21:00">18:00 - 21:00</option>
                <option value="21:00 - 09:00">21:00 - 09:00</option>
                </select>
            </div>
          </div>
      
          <div class="form-group text optional service_ac_request_question11">
              <label class="control-label text optional" for="service_ac_request_question11">Informasi tambahan</label><div><textarea class="ampstart-input" rows="4" name="service_ac_request[question11]" id="service_ac_request_question11" style="overflow: hidden; word-wrap: break-word; resize: horizontal; height: 98px;"></textarea><p class="help-block">Jika Anda menjawab ‘lainnya’ sebagai jawaban pada pertanyaan-pertanyaan sebelumnya, harap menjelaskan dengan lengkap di sini. Semakin lengkap informasi yang Anda berikan, semakin akurat penawaran harga yang Anda dapatkan</p></div>
              
              <input class="form-control hidden" value="-" type="hidden" name="service_ac_request[quote_request_attributes][payment_option]" id="service_ac_request_quote_request_attributes_payment_option">
          </div>
        </section>
      
        <section class="px3 mb4">
          <h2 class="">Kontak Anda</h2>
      
          <div class="ampstart-input inline-block relative m0 p0 mb3 ">
            <input type="text" value="" name="service_ac_request[quote_request_attributes][name]" id="ip1" class="block border-none p0 m0" placeholder="Nama" required>
            
            <label for="ip1" class="absolute top-0 right-0 bottom-0 left-0" aria-hidden="true">
                Nama
            </label>
          </div>
      
          <div class="ampstart-input inline-block relative m0 p0 mb3 ">
            <input type="email" value="" name="service_ac_request[quote_request_attributes][email]" id="ip1" class="block border-none p0 m0" placeholder="Email" required>
            <label for="ip1" class="absolute top-0 right-0 bottom-0 left-0" aria-hidden="true">
                Email
            </label>
          </div>
      
          <!-- <div class="ampstart-input inline-block relative m0 p0 mb3 ">
            <input type="text" value="82" name="service_ac_request[quote_request_attributes][area_id]" id="ip1" class="block border-none p0 m0" placeholder="Provinsi">
            <label for="ip1" class="absolute top-0 right-0 bottom-0 left-0" aria-hidden="true">
                Provinsi
            </label>
          </div>
          
          <div class="ampstart-input inline-block relative m0 p0 mb3 ">
            <input type="text" value="4386"  name="service_ac_request[quote_request_attributes][city_id]" id="ip1" class="block border-none p0 m0" placeholder="Kota">
            <label for="ip1" class="absolute top-0 right-0 bottom-0 left-0" aria-hidden="true">
                Kota
            </label>
          </div> -->
      
          <div class="ampstart-input inline-block relative m0 p0 mb3 ">
            <textarea name="service_ac_request[quote_request_attributes][kecamatan]" id="ip13" class="block border-none  p0 m0" rows="5"></textarea>
            <label for="ip13" class="absolute top-0 right-0 bottom-0 left-0" aria-hidden="true" required>
                Kecamatan
            </label>
          </div>
      
      
          <div>
            <amp-list height="25" layout="fixed-height" src="https://gist.githubusercontent.com/zainalmustofa/0c35a9f2769567e736ba0737d7cab666/raw/0eb5adb119d4daeb5f3a6fd698d73b988a015022/tesing2.json">
              <template type="amp-mustache">
                <label for="country">Provinsi:</label>
                <select id="country" on="
                  change:
                    AMP.setState({
                      cities: dropdown.items[0].countries.filter(x => x.id == event.value)
                    })"
                >
                  <option value="">Pilih Provinsi</option>
                  {{#countries}}
                    <option value="{{id}}">{{name}}</option>
                  {{/countries}}
                </select>
              </template>
            </amp-list>
      
            <amp-list height="25" layout="fixed-height" [src]="cities" src="https://gist.githubusercontent.com/zainalmustofa/0c35a9f2769567e736ba0737d7cab666/raw/0eb5adb119d4daeb5f3a6fd698d73b988a015022/tesing2.json">
              <template type="amp-mustache">
                <label for="city">Kota:</label>
                <select [disabled]="!cities" disabled id="city" on="change:AMP.setState({city: event.value})">
                  {{^cities}}<option value="">Pilih Kota</option>{{/cities}}
                  {{#cities.0}}<option value="">Pilih Kota</option>{{/cities.0}}
                  {{#cities}}<option value="{{id}}">{{name}}</option>{{/cities}}
                </select>
              </template>
            </amp-list>
      
            <amp-state id="dropdown" src="https://gist.githubusercontent.com/zainalmustofa/0c35a9f2769567e736ba0737d7cab666/raw/0eb5adb119d4daeb5f3a6fd698d73b988a015022/tesing2.json"></amp-state>
          </div>
          
          <input type="hidden" id="cityId" name="service_ac_request[quote_request_attributes][city_id]"  [value]="city.split(',')[0]" name="service_ac_request[quote_request_attributes][city_id]">
      
          <div class="ampstart-input inline-block relative m0 p0 mb3 ">
            <input type="text" value="" name="service_ac_request[quote_request_attributes][phone]" id="ip1" class="block border-none p0 m0" placeholder="No Handphone" required>
            <label for="ip1" class="absolute top-0 right-0 bottom-0 left-0" aria-hidden="true" required>
                No Handphone
            </label>
          </div>
      
          <div class="ampstart-input inline-block relative m0 p0 mb3 ">
            <input type="text" value="" name="service_ac_request[quote_request_attributes][referal_code]" id="ip1" class="block border-none p0 m0" placeholder="Kode Promo">
            <label for="ip1" class="absolute top-0 right-0 bottom-0 left-0" aria-hidden="true">
                Kode Promo
            </label>
          </div>
